generator client {
  provider     = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  directUrl    = env("DIRECT_URL")
  relationMode = "prisma"
}

model contentmeta {
  id          Int      @id @default(autoincrement())
  slug        String   @unique
  type        String
  views       Int      @default(0)
  updated_at  DateTime @default(now())
}
