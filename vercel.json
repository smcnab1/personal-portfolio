{
  "functions": {
    "src/pages/api/chat.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "DATABASE_URL": "file:./prisma/dev.db"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=60, stale-while-revalidate=30"
        }
      ]
    }
  ]
}
